{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "convex dev --once && node setup.mjs && npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "build": "vite build",
    "deploy:convex": "npx convex deploy",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "test": "vitest run"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.80",
    "@react-spring/web": "^10.0.1",
    "animejs": "^4.0.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "color-thief-react": "^2.1.0",
    "convex": "^1.24.6",
    "date-fns": "^4.1.0",
    "form-data": "^4.0.2",
    "framer-motion": "^12.16.0",
    "gsap": "^3.13.0",
    "lottie-react": "^2.4.1",
    "openai": "^4.104.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-force-graph-2d": "^1.27.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "twilio": "^5.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/animejs": "^3.1.13",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-jsx": "^2.2.9",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "~10",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.3"
  }
